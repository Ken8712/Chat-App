<div class="side__upper">
  <div class="side__upper-name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="side__upper-linkbox">
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>
<div class="side__lower">
  <% current_user.rooms.each do |room| %>
    <div class="side__lower-roombox">
      <div class="room-name">
        <%= link_to room.name, room_messages_path(room) %>
      </div>
    </div>
  <% end %>
</div>